{
    "editor.fontSize": 13,
    "workbench.colorTheme": "Material Theme Lighter High Contrast",
    "breadcrumbs.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.tokenColorCustomizations": {
        "comments": "#8FBC8B",
      //  "variables": "#2F4F4F",
        "textMateRules": [
            {
                "scope": [
                    "source",
                    "variable.other.property",
                    "variable.other.object.property"
                ],
                "settings": {
                    "foreground": "#708090",
                }
            },
            {
                "scope": [
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end",
                    "string.quoted.double",
                    "string.quoted.single",
                    "constant.numeric"
                ],
                "settings": {
                    "foreground": "#DC143C",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "meta.tag.without-attributes.tsx meta.jsx.children.tsx",
                    "meta.tag.without-attributes.tsx meta.jsx.children.ts",
                ],
                "settings": {
                    "foreground": "#DC143C",
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name.tsx",
                    "entity.other.attribute-name.ts",
                ],
                "settings": {
                    "foreground": "#1E90FF"
                }
            },
            {
                "scope": [
                    "punctuation.definition.tag.begin",
                    "punctuation.definition.tag.end",
                    "entity.name.tag",
                    "support.class.component"
                ],
                "settings": {
                    "foreground": "#FF8C00",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "support.class.component.tsx",
                    "support.class.component.ts",
                ],
                "settings": {}
            },
            // #FF69B4 
            {
                "scope": [
                    "variable.language.this.tsx",
                    "variable.language.this.ts",
                ],
                "settings": {
                    "foreground": "#FF69B4",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "meta.class meta.field.declaration",
                    "meta.class meta.definition.property",
                    "meta.definition.property variable.object.property"
                ],
                "settings": {
                    "foreground": "#FF69B4"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                ],
                "settings": {
                    "foreground": "#000000"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "support.function",
                ],
                "settings": {
                    "foreground": "#6f42c1"
                }
            },
            {
                "scope": [
                    "keyword",
                    "keyword.control",
                    "keyword.operator.new",
                    "keyword.operator.expression.instanceof",
                    "constant.language.null",
                    "storage",
                    "variable.language.super",
                    "storage.modifier",
                    "storage.type.class",
                    "storage.type.tsx",
                    "storage.type.ts",
                    "storage.modifier.async",
                    "storage.type",
                    "storage.type.interface.tsx",
                    "storage.type.interface.ts",
                    "storage.modifier.tsx",
                    "storage.modifier.ts",
                    "storage.type.interface.tsx",
                    "storage.type.interface.ts",
                    "storage.type.function.tsx",
                    "storage.type.function.ts",
                ],
                "settings": {
                    "foreground": "#2F4F4F",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                    "meta.brace",
                    "punctuation.definition.block",
                    "punctuation.separator.key-value",
                    "punctuation.definition.parameters.begin",
                    "punctuation.definition.parameters.end"
                ],
                "settings": {
                    "foreground": "#2F4F4F",
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "support.type",
                    "entity.name.type",
                    "entity.other.inherited-class.tsx",
                    "entity.other.inherited-class.ts",
                    "support.class.builtin"

                ],
                "settings": {
                    "foreground": "#008B8B",
                }
            },
            {
                "scope": [
                    "variable.other.readwrite.alias",
                    "variable.other.readwrite",
                    "variable.other.object",
                    "variable.parameter",
                    "support.class.console",
                    "support.variable.dom",
                    "support.variable.property.dom"
                ],
                "settings": {
                    "foreground": "#808000",
                }
            },
        ]
    },
    "mssql.connections": [
        {
            "server": "green",
            "database": "wms2019",
            "authenticationType": "SqlLogin",
            "user": "sa",
            "password": "",
            "emptyPasswordInput": true,
            "savePassword": true
        }
    ],
    "git.enableSmartCommit": true
}